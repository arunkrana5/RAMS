@{
    Layout = "_Layout";
}
<link href="https://cdn.datatables.net/2.1.4/css/dataTables.dataTables.css" rel="stylesheet" />
<style>
    table td:last-child {
        text-align: start;
    }
</style>
<div class="d-flex mb-10 flex-wrap justify-content-between align-items-center">
    <h6 class="page-title">Employee List</h6>
    <div class="d-flex flex-wrap justify-content-end gap-2 align-items-center breadcrumb-plugins">
        <a href="@Url.Action("AddUser","Master",new {Area="SecureZone"})" class="btn btn-sm btn--primary"><i class="las la-plus"></i>Add</a>
        <a onclick="history.back()" class="btn btn-sm btn--primary">
            <i class="la la-undo"></i> Back
        </a>
    </div>
</div>
<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="table-responsive">
                <table class="table align-items-center mb-0 stripe" id="user_table">
                    <thead class="thead-dark">
                        <tr>
                            <th>Action</th>
                            <th>Employee Code</th>
                            <th>Employee Name</th>
                            <th>Dealer Name</th>
                            <th>Dealer Code</th>
                            <th>DOL Status</th>
                            <th>Phone</th>
                            <th>Email ID</th>
                            <th>Father Name</th>
                            <th>DOB</th>
                            <th>Gender</th>
                            <th>Designation</th>
                            <th>Department</th>
                        </tr>
                    </thead>
                    <tbody id="tbody_Users"></tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="https://cdn.datatables.net/2.1.4/js/dataTables.js"></script>
<script src="~/assets/js/custom/userlist.js"></script>



