@{
    Layout = "_Layout";
}
<link href="https://cdn.datatables.net/2.1.4/css/dataTables.dataTables.css" rel="stylesheet" />
<style>
    table.dataTable th.dt-type-numeric, table.dataTable th.dt-type-date, table.dataTable td.dt-type-numeric, table.dataTable td.dt-type-date {
        text-align: left;
    }

    table td:last-child {
        text-align: left;
    }

    .btn--primary {
        background-color:transparent;
    }
</style>
<div class="d-flex mb-10 flex-wrap gap-3 justify-content-between align-items-center">
    <h6 class="page-title" id="headerTitle">PJP Plan List</h6>
    <div class="d-flex flex-wrap justify-content-end gap-2 align-items-center breadcrumb-plugins">
        <a href="@Url.Action("PJP","Transaction",new {Area="SecureZone"})" class="btn btn-sm btn--primary">
            <i class="las la-plus"></i>Add
        </a>
        <a onclick="history.back()" class="btn btn-sm btn--primary">
            <i class="la la-undo"></i> Back
        </a>
    </div>
</div>
<div class="card">
    <div class="card-body">
        <div class="row">
            <div class="table-responsive">
                <table class="table align-items-center mb-0 stripe" id="tbl_PJPPlan">
                    <thead class="thead-dark">
                        <tr>
                            <th>Action</th>
                            <th>Plan Date</th>
                            <th>Visit Date</th>
                            <th>Employee</th>
                            <th>Route Number</th>
                            <th>Route Type</th>
                            <th>Vendor</th>
                            <th>Created By</th>
                            <th>Created Date</th>
                            <th>Modified By</th>
                            <th>Modified Date</th>
                            <th>IPAddress</th>
                        </tr>
                    </thead>
                    <tbody id="tbody_PJPPlan">
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
<script src="https://cdn.datatables.net/2.1.4/js/dataTables.js"></script>
<script src="~/assets/js/custom/pjpplanlist.js"></script>